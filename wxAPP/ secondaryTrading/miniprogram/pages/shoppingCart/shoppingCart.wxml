<!--miniprogram/pages/shoppingCart/shoppingCart.wxml-->
<van-submit-bar
  price="{{ 0 }}"
  button-text="提交订单"
  bind:submit="onClickButton"
  tip="{{ true }}"
>
  <van-checkbox value="{{ checked }}" bind:change="onChange">全选</van-checkbox>
  <view slot="tip">您的收货地址不支持同城送, <text>修改地址</text></view>
</van-submit-bar>
