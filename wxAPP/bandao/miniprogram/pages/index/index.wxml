<block wx:for="{{products}}" wx:key="index">
  <van-button type="default" bindtap="upload">默认按钮</van-button>
  <view>{{item.title}}</view>
  <view>{{item.view}}</view>
  <view>{{item.color}}</view>
  <image src="{{item.image}}"/>
</block>
<block wx:for="{{photos}}" wx:key="index">
  <image class="" src="{{item.image}}"/>   
</block>
  