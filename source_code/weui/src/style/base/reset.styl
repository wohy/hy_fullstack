@import "fn"

html 
    -ms-text-size-adjust 100%
    // ms为微软前缀，控制字体大小
    -webkit-text-size-adjust 100%

body
    line-height 1.6
    font-family $weuiFontDefault
*
    margin 0
    padding 0
a img 
    border 0
a
    text-decoration none
    setTapColor()

input, textarea
    caret-color $weuiColorPrimary            